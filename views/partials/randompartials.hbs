<div class='random-img'>
  <img src={{beer.image_url}} alt={{beer.Title}} />
</div>

<div class='random-beer'>
  <h3>{{beer.name}}</h3>
  <p>{{beer.dscription}}</p>
  <q>{{beer.tagline}}</q>

  <h4>Food Pairings</h4>
  <p>Brewer Tips:
    {{beer.brewers_tips}}</p>
  <ul>
    {{#each beer.food_pairing}}

      <li>{{this}}</li>

    {{/each}}
  </ul>
</div>